<?php

namespace App\EducationBundle\Entity;

/**
 * ImageProduct
 */
class ImageProduct
{

    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $image_product;

    /**
     * @var \App\EducationBundle\Entity\Product
     */
    private $product;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set imageProduct
     *
     * @param string $imageProduct
     *
     * @return ImageProduct
     */
    public function setImageProduct($imageProduct)
    {
        $this->image_product = $imageProduct;

        return $this;
    }

    /**
     * Get imageProduct
     *
     * @return string
     */
    public function getImageProduct()
    {
        return $this->image_product;
    }

    /**
     * Set product
     *
     * @param \App\EducationBundle\Entity\Product $product
     *
     * @return ImageProduct
     */
    public function setProduct(\App\EducationBundle\Entity\Product $product = null)
    {
        $this->product = $product;

        return $this;
    }

    /**
     * Get product
     *
     * @return \App\EducationBundle\Entity\Product
     */
    public function getProduct()
    {
        return $this->product;
    }
    /**
     * @var string
     */
    private $description;


    /**
     * Set description
     *
     * @param string $description
     *
     * @return ImageProduct
     */
    public function setDescription($description)
    {
        $this->description = $description;

        return $this;
    }

    /**
     * Get description
     *
     * @return string
     */
    public function getDescription()
    {
        return $this->description;
    }
}
